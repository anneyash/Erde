<div class="col-md-12 form-progress-bar">
  <div class="progress" style="height: 1px;">
    <div class="progress-bar" role="progressbar" style="width: 60%;" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
  </div>
  <div class="progress" style="height: 9px;">
    <div class="progress-bar" role="progressbar" style="width: 60%;" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
  </div>
</div>

<div class="col-md-7">
  <form>

    <div class="form-group visa-applications-form">
      <h6>
        <strong>Applicant's home address and e-mail address</strong>
      </h6>
      <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in applicant's home address and e-mail address">
    </div>

    <div class="form-group visa-applications-form">
      <h6>
        <strong>Telephone number(s)</strong>
      </h6>
      <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type telephone number(s)">
    </div>

    <div class="form-group visa-applications-form">
      <h6>
        <strong>Residence in a country other than the country of current nationality</strong>
      </h6>
      <label><input type="radio" name="radio" class="as-checkbox">Yes</label>
      <label><input type="radio" name="radio" class="as-checkbox">No</label>
      <small id="passwordHelpBlock" class="form-text text-muted">Residence permit or equivalent №</small>
      <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Residence permit or equivalent №">
      <small id="passwordHelpBlock" class="form-text text-muted">Valid until</small>

      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />
      <div class="dates">
        <div class="start_date input-group mb-4">
          <input class="form-control start_date" type="date" id="startdate_datepicker">
        </div>
      </div>
    </div>


    <div class="form-group visa-applications-form">
      <h6>
        <strong>Current occupation</strong>
      </h6>
      <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type current occupation">
    </div>

    <div class="form-group visa-applications-form">
      <h6>
        <strong>Employer and employer's address and telephone number.</strong>
        <small class="text-muted help-text">For students, name and address establishment.</small>
      </h6>
      <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type employer address and telephone number(s)">
    </div>

    <%= link_to 'Далее', visa_applications_path(:step => "14"), class: "btn btn-primary"%>

    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>

  </form>
</div>
