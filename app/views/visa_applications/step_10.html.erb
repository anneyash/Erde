<div class="col-md-12 form-progress-bar">
  <div class="progress" style="height: 1px;">
    <div class="progress-bar" role="progressbar" style="width: 15%;" aria-valuenow="15" aria-valuemin="0" aria-valuemax="100"></div>
  </div>
  <div class="progress" style="height: 9px;">
    <div class="progress-bar" role="progressbar" style="width: 15%;" aria-valuenow="15" aria-valuemin="0" aria-valuemax="100"></div>
  </div>
</div>

<div class="col-md-7">
<form>
  <div class="form-group visa-applications-form">
    <h6>
      <strong>Surname</strong>
      <small class="text-muted">(Family name)</small>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in your surname">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Surname at birth</strong>
      <small class="text-muted">(Former family name(s))</small>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in surname at birth">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>First name(s)</strong>
      <small class="text-muted">(Given name(s))</small>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in first surname(s)">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Date of birth</strong>
    </h6>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />
    <div class="dates">
      <div class="start_date input-group mb-4">
        <input class="form-control start_date" type="date" id="startdate_datepicker">
      </div>
    </div>
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Place of birth</strong>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in the place of birth">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Country of birth</strong>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in the country of birth">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Current nationality</strong>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in the current nationality">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Nationality at birth</strong>
      <small class="text-muted">(if different)</small>
    </h6>
    <input type="textanswer" class="form-control" id="FormControlInput1" placeholder="Type in the nationality at birth">
  </div>

  <div class="form-group visa-applications-form">
    <h6>
      <strong>Sex</strong>
    </h6>
  </div>
  <div class="custom-control custom-checkbox custom-control-inline">
    <input type="checkbox" id="customCheckInline1" name="customCheckInline1" class="custom-control-input">
    <label class="custom-control-label" for="customCheckInline1">Male</label>
  </div>
  <div class="custom-control custom-checkbox custom-control-inline">
    <input type="checkbox" id="customCheckInline2" name="customCheckline2" class="custom-control-input">
    <label class="custom-control-label" for="customCheckInline2">Female</label>
  </div>

  <br>
  <br>
  <br>
  <br>

  <%= link_to 'Далее', visa_applications_path(:step => "11"), class: "btn btn-primary"%>

  <br>
  <br>
  <br>
  <br>
  <br>
  <br>
  <br>

</form>
</div>
