<p id="notice"><%= notice %></p>

<h1>Visa Application Forms</h1>

<table>
  <thead>
    <tr>
      <th>Country</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visa_application_forms.each do |visa_application_form| %>
      <tr>
        <td><%= visa_application_form.country_id %></td>
        <td><%= visa_application_form.title %></td>
        <td><%= link_to 'Show', visa_application_form %></td>
        <td><%= link_to 'Edit', edit_visa_application_form_path(visa_application_form) %></td>
        <td><%= link_to 'Destroy', visa_application_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visa Application Form', new_visa_application_form_path %>
