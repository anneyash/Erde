<p id="notice"><%= notice %></p>

<h1>Visa Application Form Steps</h1>

<table>
  <thead>
    <tr>
      <th>Visa application form</th>
      <th>Step</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visa_application_form_steps.each do |visa_application_form_step| %>
      <tr>
        <td><%= visa_application_form_step.visa_application_form_id %></td>
        <td><%= visa_application_form_step.step_id %></td>
        <td><%= visa_application_form_step.position %></td>
        <td><%= link_to 'Show', visa_application_form_step %></td>
        <td><%= link_to 'Edit', edit_visa_application_form_step_path(visa_application_form_step) %></td>
        <td><%= link_to 'Destroy', visa_application_form_step, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visa Application Form Step', new_visa_application_form_step_path %>
